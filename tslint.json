{
  "rules": {
    "jsx-wrap-multiline": false,
    "semicolon": [true, "never"],
    "class-name": true,
    "import-spacing": true,
    "new-parens": true,
    "eofline": true,
    "no-boolean-literal-compare": [true],
    "no-consecutive-blank-lines": true,
    "no-trailing-whitespace": true,
    "quotemark": [true, "single", "jsx-double"]
  }
}